{
  "name": "autumhire",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --outDir docs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
